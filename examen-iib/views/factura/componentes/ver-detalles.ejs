<% console.log('aqui',datos) %> 
<table class="table">
    <thead>
        <tr>
            <th scope="col">Descripcion</th>
            <th scope="col">Cantidad</th>
            <th scope="col">Precio U</th>
            <th scope="col">Subtotal</th>
        </tr>
    </thead>
    <tbody>
        <%datos.detalles.forEach( detalle => {%>
        <tr>
            <td>
                <%= detalle.pareja.nombre%>
            </td>
            <td>
                <%= detalle.cantidad%>
            </td>
            <td>
                <%= detalle.precio%>
            </td>
            <td>
                <%= detalle.subtotal%>
            </td>
        </tr>
        <%})%>
    </tbody>
</table>